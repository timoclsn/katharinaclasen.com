{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["SANITY_PROJECT_ID", "SANITY_AUTH_TOKEN", "DRAFT_MODE_SECRET"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    }
  }
}
